{
    "name":              "insolita/unused-scanner" ,
    "description":       "Detect unused composer dependencies" ,
    "type":              "library" ,
    "license":           "mit" ,
    "authors":           [
        {
            "name":  "insolita" ,
            "email": "webmaster100500@ya.ru"
        }
    ] ,
    "minimum-stability": "dev" ,
    "require":           {
        "php":            ">=7.2" ,
        "ext-json":       "*" ,
        "symfony/finder": "^3.4|^4.0" ,
        "nesbot/carbon":  ">=1.22"
    } ,
    "require-dev":       {
        "phpunit/phpunit": "^7.0|^8.0" ,
        "symfony/thanks":  "1.0.*"
    } ,
    "bin":               [
        "unused_scanner"
    ] ,
    "autoload":          {
        "psr-4": {
            "insolita\\Scanner\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-legacy": "1.3.x-dev"
        }
    }
}
